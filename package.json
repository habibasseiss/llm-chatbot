{
  "name": "llm-chatbot",
  "version": "0.0.1",
  "description": "A chatbot with AI capabilities",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/src/main.js",
    "build": "tsc && tsc-alias",
    "dev": "tsx src/main.ts",
    "cli": "ENABLED_SOURCES=cli tsx src/main.ts",
    "whatsapp": "ENABLED_SOURCES=whatsapp tsx src/main.ts",
    "test": "jest",
    "migrate": "node-pg-migrate --decamelize true",
    "seed": "node seeds/seed-runner.js",
    "db:setup": "npm run migrate up && npm run seed",
    "create:seed": "node scripts/create-seed.js"
  },
  "author": "Habib Asseiss",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "node-pg-migrate": "^7.6.1",
    "ollama": "^0.5.11",
    "openai": "^4.85.4",
    "pg-promise": "^11.9.1",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1",
    "zod": "^3.24.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [],
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.1",
    "@types/pg": "^8.11.8",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "overrides": {
    "node-fetch@2.x": {
      "whatwg-url": "14.x"
    }
  }
}
